{
  "name": "compasscad",
  "version": "1.1.0",
  "description": "very good CAD software",
  "main": "entry.js",
  "author": "zeankun.dev",
  "license": "UFOL-2.0",
  "homepage": "https://zeankundev.github.io/CompassCAD",
  "scripts": {
    "start": "electron .",
    "start:no-sandbox": "electron . --no-sandbox",
    "instant-build": "electron-builder && electron-builder --win"
  },
  "private": false,
  "dependencies": {
    "@electron/remote": "^2.0.9",
    "jquery": "^3.7.1"
  },
  "build": {
    "appId": "dev.zeankun.compasscad",
    "productName": "CompassCAD",
    "copyright": "Copyright Â© 2024 zeankun.dev",
    "icon": "build/icons/512x512.png",
    "directories": {
      "output": "dist"
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ],
      "publisherName": "zeankun.dev"
    },
    "linux": {
      "target": [
        "deb",
        "AppImage",
        "snap",
        "rpm"
      ],
      "maintainer": "zeankun.dev",
      "category": "Graphics",
      "icon": "./build/icons/512x512.png"
    },
    "snap": {
    	"allowNativeWayland": true
    },
    "mac": {
      "target": [
        "zip",
        "tar.gz"
      ],
      "category": "public.app-category.graphics-design",
      "icon": "./build/icons/512x512.png"
    }
  },
  "devDependencies": {
    "electron": "^31.1.0",
    "electron-builder": "^24.6.3",
    "electron-reloader": "^1.2.3"
  }
}
